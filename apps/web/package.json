{
  "name": "coffee-ai-mentor-web",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "24.x"
  },
  "scripts": {
    "dev": "next dev --hostname 127.0.0.1 --experimental-https",
    "dev:docker": "next dev --hostname 0.0.0.0 --port 3000 --experimental-https --experimental-https-key ../../certificates/dev-key.pem --experimental-https-cert ../../certificates/dev-cert.pem",
    "dev:emu": "firebase emulators:exec --only auth \"next dev --hostname 127.0.0.1 --experimental-https --experimental-https-key ../../certificates/dev-key.pem --experimental-https-cert ../../certificates/dev-cert.pem\"",
    "build": "next build",
    "start": "next start",
    "build:standalone": "next build && mkdir -p .next/standalone/apps/web/.next && cp -r public .next/standalone/apps/web/ && cp -r .next/static .next/standalone/apps/web/.next/",
    "start:standalone": "HOSTNAME=127.0.0.1 PORT=3000 node --env-file=.env.local .next/standalone/apps/web/server.js",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "format": "biome format --write",
    "test": "firebase emulators:exec --only auth \"vitest --ui --coverage\"",
    "test:ci": "firebase emulators:exec --only auth \"vitest\"",
    "test:unit": "vitest --ui --project unit",
    "test:unit:ci": "vitest --project unit",
    "test:integration": "firebase emulators:exec --only auth \"vitest --project integration --ui\"",
    "test:integration:ci": "firebase emulators:exec --only auth \"vitest --project integration\"",
    "test:e2e": "firebase emulators:exec --only auth \"playwright test --ui\"",
    "test:e2e:ci": "firebase emulators:exec --only auth \"playwright test\""
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@sentry/nextjs": "^10.39.0",
    "@t3-oss/env-nextjs": "^0.13.10",
    "@tanstack/react-query": "^5.90.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "esbuild": "^0.27.3",
    "firebase": "^12.9.0",
    "firebase-admin": "^13.6.1",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "next-themes": "^0.4.6",
    "pino": "^10.3.1",
    "radix-ui": "^1.4.3",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-hook-form": "^7.71.2",
    "sharp": "^0.34.5",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.5.0",
    "unrs-resolver": "^1.11.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "^4.2.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.13",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "babel-plugin-react-compiler": "1.0.0",
    "client-only": "^0.0.1",
    "dotenv": "^17.3.1",
    "execa": "^9.6.1",
    "fetch-cookie": "^3.2.0",
    "firebase-tools": "^15.7.0",
    "jsdom": "^27.4.0",
    "msw": "^2.12.10",
    "pino-pretty": "^13.1.3",
    "prisma": "^7.4.1",
    "server-only": "^0.0.1",
    "tailwindcss": "^4.2.0",
    "tough-cookie": "^6.0.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^6.1.1",
    "vitest": "^4.0.18"
  },
  "packageManager": "pnpm@10.29.2+sha512.bef43fa759d91fd2da4b319a5a0d13ef7a45bb985a3d7342058470f9d2051a3ba8674e629672654686ef9443ad13a82da2beb9eeb3e0221c87b8154fff9d74b8"
}
